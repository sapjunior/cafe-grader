= user_title_bar(@user)

.task-menu
  %b Task Listing:

  - @problems.each do |problem|
    - if problem.description!=nil
      %a{:href => "\##{problem.name}"}= "[#{problem.full_name}]"
    - else
      = "[#{problem.full_name}]"

%table.taskdesc
  = render :partial => 'problem', :collection => @problems
