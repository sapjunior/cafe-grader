<div class="title">Hello <%=h @user.full_name %></div>
Current time is <%= format_short_time(Time.new) %>.

<div class="submitbox">
  <%= error_messages_for 'submission' %>
  <%= render :partial => 'submission_box' %>
</div>

<hr/>

<table class="info">
  <tr class="info-head">
    <th class="info"></th>
