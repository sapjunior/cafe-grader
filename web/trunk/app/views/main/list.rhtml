    <th class="info"># of sub(s)</th>
    <th class="info">Results</th>
  </tr>
  <%= render :partial => 'problem', :collection => @problems %>
</table>

<hr />

<div class="submitbox">
  <%= render :partial => 'submission_box' %>
</div>

