problem do
  num_tests <%= num_testcases %>
  full_score <%= num_testruns*10 %>
  time_limit_each <%= options[:time_limit] %>
  mem_limit_each <%= options[:mem_limit] %>
  score_each 10

<% tr_num = 0 %>
<% testrun_info.each do |testrun| %>
  <% tr_num += 1 %>
  run <%= tr_num %> do
    tests <%= (testrun.collect {|testcase| testcase[0]}).join(", ") %>
    <% if testrun.length==1 %>
    scores 10
    <% else %>
    scores 10 <% (testrun.length-1).times do %>,10 <% end %>
    <% end %>
  end
<% end %>
end
