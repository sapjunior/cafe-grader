%h1 Import data

= content_tag(:div, flash[:notice], :class => "notice") if flash[:notice]!=nil

.menu
  = link_to "[return]", :action => 'list'
  to country list

.new-box
  - form_tag({:action => 'import_data'}, :multipart => true) do
    %b Upload data:
    = file_field_tag :file
    = submit_tag "Upload"
  %br/
  %small
    File should be a YAML file.
